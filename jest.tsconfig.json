{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "sourceMap": false,
        "experimentalDecorators": true,
        "noImplicitUseStrict": true,
        "removeComments": true,
        "esModuleInterop": true, 
        "moduleResolution": "node",
        "lib": [
            "es2017",
            "dom"
        ],
        "typeRoots": [
            "node_modules/@types"
        ]
    },
    "exclude": [
        "node_modules",
        "out",
        ".next"
    ]
  }